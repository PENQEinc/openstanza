  .data_table > :nth-child(1) {
    border: solid 2px #C1C6D3;
    border-radius: 5px;
  }
  .data_name > h2, h3, p{
    margin: 0;
  }
  .data_name > h3 {
    font-size: 12px;
    font-weight: normal;
  }
  .data_name > h2 > ruby {
    font-size: 24px;
  }
  .data_name > h2 > ruby > rt {
    font-size: 8px;
    font-weight: normal;
  }
  .data_name > h2 > h3 {
    font-size: 14px;
    font-weight: normal;
  }
  .data_info1 > div > p {
    border: 1px solid;
    border-radius: 3px;
  }

  .data_table > :nth-child(1) > div  {
    padding: 10px;
  }
  .data_table > :nth-child(2) {
    padding-top: 10px;
  }

  .data_name {
    border-bottom: 1px dotted #C1C6D3;
    margin: 0;
  }
  .link-wrapper {
    border-bottom: none;
  }
  .data_info1, .data_info2 {
    display: flex;
  }
  .data_info1 > div {
    display: inline;
    border: #147786 1px solid;
    border-radius: 3px;
    margin-right: 10px;
    font-size: 12px;
    padding: 4px 10px;
    background-color: #147786;
    color: #fff;
  }

  .data_info1 > div > a {
    color: #fff;
    text-decoration: none;
  }
  
  .data_info2 > div {
    display: inline;
    font-weight: bold;
    background-color: rgba(20,119,134,0.2);
    border-radius: 3px;
    margin-right: 2px;
    font-size: 10px;
    padding: 2px 4px 6px;
  }
  .data_info2 > div > div {
    margin-top: 6px;
  }

  .data_info2 > div > div > span{
    border: rgb(20,119,134) 1px solid;
    border-radius: 3px;
    padding: 4px 20px;
    font-size: 12px;
    background-color: rgb(20,119,134);
  }

  .data_info2 > div > div > span> a{
    color: #fff;
    text-decoration: none;
  }

  details .data-list {
    display: flex;
    margin-top: 5px;
  }

  .data-list > div {
    background-color: rgba(20,119,134,0.2);
    border-radius: 3px;
    margin-right: 2px;
    font-size: 10px;
    padding: 2px 4px 6px;
  }

  .data-list > div > h3 {
    font-size: 10px;
    font-weight: normal;
  }

  .data-list > div > div {
    border: rgb(20,119,134) 1px solid;
    border-radius: 3px;
    padding: 4px 20px;
    background-color: rgb(20,119,134);
  }

  .data-list > div > div > p > a{
    font-size: 12px;
    font-weight: normal;
    color: #fff;
    text-decoration: none;
  }

  details summary {
    border-bottom: none;
  }

  .tab-wrap {
  display: flex;
  flex-wrap: wrap;
  margin: 12px 0;
  }
  .tab-wrap:after {
    content: '';
    width: 100%;
    height: 2px;
    background: #13295A;;
    display: block;
    order: -1;
  }
  .tab-label {
    white-space: nowrap;
    text-align: center;
    padding: 6px 16px;
    order: -1;
    position: relative;
    cursor: pointer;
    border-radius: 5px 5px 0 0;
    color: #13295A;
    font-weight: bold;
  }

  .tab-content {
    width: 100%;
    height: 0;
    overflow: hidden;
    opacity: 0;
  }
  .tab-switch:checked+.tab-label {
    border: #13295A 2px solid;
    border-bottom: #fff 2px solid;
    margin-bottom: -2px;
  }
  .tab-switch:checked+.tab-label+.tab-content {
    height: auto;
    overflow: auto;
    padding: 10px 5px;
    opacity: 1;
    transition: .5s opacity;
  }
  .tab-switch {
    display: none;
  }
  details {
    z-index: 0;
    position: relative;
  }

  #data-wrapper details summary {
    color: #fff;
  }
  #data-wrapper details summary:before {
    width: 100%;
    height: 28px;
    z-index: -1;
    position: absolute;
  }
  #data-wrapper details[open] summary:after {
    left: 7px;
  }
  #data-wrapper details summary:after {
    left: 9px;
  }

  .phenotype_view {
    display: flex;
    flex-wrap: wrap;
    flex-direction: column;
    height: 550px;
  }

  .phenotype_view > div > ul {
    padding: 0 5px;
    list-style: none;
    font-size: 14px;
    font-weight: bold;
  }
  .phenotype_view > div > ul > li {
    font-weight: normal;
    padding-top: 4px;
  }
  .checkbox{
  display: none;
  }
  /*アコーディオンの大きさと位置調整*/
  .hierarchy_view {
    margin-left: 20px;
  }
  .hierarchy_view label{
    position: relative;
    display: block;
    cursor: pointer;
  }
  
  /*ラベル内の矢印の設定*/
  .hierarchy_view label::before{
    content:'';
    display: block;
    border-left:5px solid transparent;
    border-right: 5px solid transparent;
    border-top: 7px solid #000;
    position: absolute;
    top: 50%;
    left: -15px;
    transform: translate(0,-50%) rotate(-90deg);
  }

  .hierarchy_view ul {
    padding: 0 5px;
  }
  
  /*表示される要素の基本設定*/
  .hierarchy_view li{
    height: 0;
    transition:.4s;
    overflow: hidden;
    list-style-type: disc;
    list-style-position: inside;
    position: relative;
  }
  .hierarchy_view li::marker {
    margin: 0;
  }
  .hierarchy_view li > span {
    text-align: right;
  }
  .hierarchy_view li > span:before {
    border-top: 1px dotted;
    content: "";
    position: absolute;
    top: 50%;
    width: 100%;
  }
  .hierarchy_view li:after {
    content: "■★△◯";
  }
  
  /*ラベル内の矢印のアニメーション設定*/
  .checkbox:checked ~ ul label::before{
    transform: translate(0,-50%);  
  }
  
  /*表示される要素のアニメーション設定*/
  .checkbox:checked ~ ul li{
    height: auto;
  }
</style>
